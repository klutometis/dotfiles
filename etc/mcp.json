{
  "mcpServers": {
    "git": {
      "command": "uvx",
      "args": ["mcp-server-git"]
    },
    "shell": {
      "command": "npx",
      "args": ["-y", "@mkusaka/mcp-shell-server"]
    },
    "context7": {
      "command": "mcp-proxy",
      "args": ["--log-level", "ERROR", "http://localhost:3100/servers/context7/sse"]
    },
    "firecrawl": {
      "command": "mcp-proxy",
      "args": ["--log-level", "ERROR", "http://localhost:3100/servers/firecrawl/sse"]
    },
    "zep": {
      "command": "mcp-proxy",
      "args": ["--log-level", "ERROR", "http://localhost:3100/servers/zep/sse"]
    },
    "perplexity": {
      "command": "mcp-proxy",
      "args": ["--log-level", "ERROR", "http://localhost:3100/servers/perplexity/sse"]
    }
  }
}
